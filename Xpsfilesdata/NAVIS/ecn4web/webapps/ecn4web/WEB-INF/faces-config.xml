<?xml version="1.0" encoding="windows-1252"?>
<!DOCTYPE faces-config PUBLIC
        "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
        "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
    <application>
        <locale-config>
            <default-locale>en_US</default-locale>
        </locale-config>
    </application>
    <navigation-rule>
        <from-view-id>/n4mobile.jsp</from-view-id>
        <navigation-case>
            <from-outcome>selectSite</from-outcome>
            <to-view-id>/siteSelection.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu</from-outcome>
            <to-view-id>/menu.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>singleProgram</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>duplicates</from-outcome>
            <to-view-id>/entrySelection.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pageEntry</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <!--shortcut rules-->
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>root</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pageEntry</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pageSubEntry</from-outcome>
            <to-view-id>/subEntry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pageDuplicates</from-outcome>
            <to-view-id>/entrySelection.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/menu.jsp</from-view-id>
        <navigation-case>
            <from-outcome>selected</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/n4mobile.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>noDuplicate</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>duplicates</from-outcome>
            <to-view-id>/entrySelection.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/program.jsp</from-view-id>
        <navigation-case>
            <from-outcome>duplicates</from-outcome>
            <to-view-id>/entrySelection.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>noDuplicate</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/n4mobile.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/menu.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/entrySelection.jsp</from-view-id>
        <navigation-case>
            <from-outcome>selected</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>stay</from-outcome>
            <to-view-id>/entrySelection.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>commit</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/n4mobile.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/entry.jsp</from-view-id>
        <navigation-case>
            <from-outcome>enterOptional</from-outcome>
            <to-view-id>/subEntry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>stay</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>submit</from-outcome>
            <to-view-id>/confirmation.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/n4mobile.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>duplicates</from-outcome>
            <to-view-id>/entrySelection.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>commit</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/siteSelection.jsp</from-view-id>
        <navigation-case>
            <from-outcome>singleProgram</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>singleProgramPageTwo</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>singleProgramDuplicates</from-outcome>
            <to-view-id>/entrySelection.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu</from-outcome>
            <to-view-id>/menu.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/n4mobile.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/confirmation.jsp</from-view-id>
        <navigation-case>
            <from-outcome>commit</from-outcome>
            <to-view-id>/program.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/n4mobile.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/subEntry.jsp</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/entry.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/n4mobile.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <lifecycle>
        <phase-listener>com.navis.framework.mobile.security.SecurityCheckListener</phase-listener>
        <phase-listener>com.navis.framework.mobile.security.SecurityMobileRoundTripStartListener</phase-listener>
        <phase-listener>com.navis.framework.mobile.security.SecurityMobileRoundTripEndListener</phase-listener>
    </lifecycle>
    <render-kit>
        <renderer>
            <component-family>variFieldFamily</component-family>
            <renderer-type>variFieldType</renderer-type>
            <renderer-class>com.navis.framework.mobile.components.varifield.HtmlVariFieldRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>selectionListFamily</component-family>
            <renderer-type>selectionListType</renderer-type>
            <renderer-class>com.navis.framework.mobile.components.selectionlist.HtmlSelectionListRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>fieldGridFamily</component-family>
            <renderer-type>fieldGridType</renderer-type>
            <renderer-class>com.navis.framework.mobile.components.fieldgrid.HtmlFieldGridRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>menuBarFamily</component-family>
            <renderer-type>menuBarType</renderer-type>
            <renderer-class>com.navis.framework.mobile.components.menubar.HtmlMenuBarRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>pageFamily</component-family>
            <renderer-type>pageType</renderer-type>
            <renderer-class>com.navis.framework.mobile.components.page.HtmlPageRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>tessInputFamily</component-family>
            <renderer-type>tessInputType</renderer-type>
            <renderer-class>com.navis.framework.mobile.components.tess.HtmlTessInputRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>selectableAreaFamily</component-family>
            <renderer-type>selectableAreaType</renderer-type>
            <renderer-class>com.navis.framework.mobile.components.selectablearea.HtmlSelectableAreaRenderer</renderer-class>
        </renderer>
        <renderer>
          <component-family>valueTableFamily</component-family>
          <renderer-type>valueTableType</renderer-type>
          <renderer-class>com.navis.framework.mobile.components.valuetable.HtmlValueTableRenderer</renderer-class>
        </renderer>
        <renderer>
          <component-family>valueTableFamily</component-family>
          <renderer-type>tessValueTableType</renderer-type>
          <renderer-class>com.navis.framework.mobile.components.valuetable.TessValueTableRenderer</renderer-class>
        </renderer>
        <renderer>
          <component-family>valueListFamily</component-family>
          <renderer-type>valueListType</renderer-type>
          <renderer-class>com.navis.framework.mobile.components.valuelist.HtmlValueListRenderer</renderer-class>
        </renderer>
        <renderer>
          <component-family>timeFamily</component-family>
          <renderer-type>timeType</renderer-type>
          <renderer-class>com.navis.framework.mobile.components.time.HtmlTimeRenderer</renderer-class>
        </renderer>
    </render-kit>
    <component>
        <component-type>varifield</component-type>
        <component-class>com.navis.framework.mobile.components.varifield.UIVariField</component-class>
    </component>
    <component>
        <component-type>selectionList</component-type>
        <component-class>com.navis.framework.mobile.components.selectionlist.UISelectionList</component-class>
    </component>
    <component>
        <component-type>fieldGrid</component-type>
        <component-class>com.navis.framework.mobile.components.fieldgrid.UIFieldGrid</component-class>
    </component>
    <component>
        <component-type>menuBar</component-type>
        <component-class>com.navis.framework.mobile.components.menubar.UIMenuBar</component-class>
    </component>
    <component>
        <component-type>selectableArea</component-type>
        <component-class>com.navis.framework.mobile.components.selectablearea.UISelectableArea</component-class>
    </component>
    <component>
        <component-type>valueTable</component-type>
        <component-class>com.navis.framework.mobile.components.valuetable.UIValueTable</component-class>
    </component>
    <component>
        <component-type>page</component-type>
        <component-class>com.navis.framework.mobile.components.page.UIPage</component-class>
    </component>
    <component>
        <component-type>tessInput</component-type>
        <component-class>com.navis.framework.mobile.components.tess.UITessInput</component-class>
    </component>
    <component>
        <component-type>valueList</component-type>
        <component-class>com.navis.framework.mobile.components.valuelist.UIValueList</component-class>
    </component>
    <component>
        <component-type>time</component-type>
        <component-class>com.navis.framework.mobile.components.time.UITime</component-class>
    </component>
    <managed-bean>
        <managed-bean-name>userEntryController</managed-bean-name>
        <managed-bean-class>com.navis.framework.mobile.bean.UserEntryController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>userEntryModel</managed-bean-name>
        <managed-bean-class>com.navis.framework.mobile.bean.UserEntryModel</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
