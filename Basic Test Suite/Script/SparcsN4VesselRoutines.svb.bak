    Set ObjCountry = Aliases.jp2launcher.n4VesselWindow.FindChild("AWTComponentName","vesCountry",20, True)
    Call CommonRoutines.EnterOperation(ObjCountry,(flowData.Item("Country")),"Country")
    Set ObjStowage = Aliases.jp2launcher.n4VesselWindow.FindChild("AWTComponentName","vesStowageScheme",20, True)
