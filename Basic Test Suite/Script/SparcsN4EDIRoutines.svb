              Set ObjFTPFolder = Aliases.jp2launcher.ObjMailBoxWindow.FindChild("AWTComponentName","edimlbxCommFolder",20,True)
              Call CommonRoutines.EnterOperation(ObjFTPFolder,flowData.Item("EDIFTPFolder"),"EDI FTP Folder")    
